/* Gramatica para expresiones aritmeticas: */

%token NUM
%token PLUS
%token MINUS
%token TIMES
%token DIVIDE
%token LPAREN
%token RPAREN
%token WS
IGNORE WS

%%
expression: 
    term
    | expression PLUS term
    | expression MINUS term
;

term: 
    factor
    | term TIMES factor
    | term DIVIDE factor
;

factor: 
    NUM
    | LPAREN expression RPAREN
;